{
    "version": 2,
    "builds": [{
        "src": "quizapp/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.10"}

}],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "quizapp/wsgi.py"
        }
    ]

}
